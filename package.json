{
  "name": "react-redux-starter",
  "version": "0.1.0",
  "description": "A boiler-plate setup for React/Redux applications",
  "license": "MIT",
  "engines": {
    "node": "4.x"
  },
  "scripts": {
    "postinstall": "npm run build",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf dist coverage",
    "start": "cross-env NODE_ENV=production node server/node-server.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "lint": "eslint .",
    "test": "npm run lint && npm run cover && npm run check-coverage",
    "test:watch": "babel-node ./node_modules/mocha/bin/_mocha --opts ./test/mocha.opts --watch",
    "cover": "babel-node ./node_modules/istanbul/lib/cli cover --report html ./node_modules/mocha/bin/_mocha -- --opts ./test/mocha.opts",
    "check-coverage": "istanbul check-coverage --lines=69"
  },
  "keywords": [
    "react",
    "redux",
    "starter",
    "seed"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rangle/react-redux-starter.git"
  },
  "bugs": {
    "url": "https://github.com/rangle/rangle-starter/issues"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "basscss": "^7.0.4",
    "chai": "^3.4.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.0",
    "cssnext-loader": "^1.0.1",
    "es5-shim": "^4.5.6",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.34.4",
    "eslint": "^2.3.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.6.3",
    "file-loader": "^0.8.4",
    "history": "^1.17.0",
    "html-webpack-plugin": "^1.7.0",
    "immutable": "^3.7.5",
    "istanbul": "^1.0.0-alpha",
    "jsdom": "^7.0.2",
    "json-loader": "^0.5.3",
    "mocha": "^2.3.3",
    "postcss-loader": "^0.8.0",
    "radium": "^0.16.5",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.2",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^2.1.0",
    "redux": "^3.0.4",
    "redux-form": "^4.1.6",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^2.1.4",
    "redux-thunk": "^1.0.0",
    "require-hacker": "^2.1.2",
    "rimraf": "^2.4.3",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.6.4"
  },
  "dependencies": {
    "express": "^4.13.3",
    "helmet": "^1.1.0",
    "http-proxy": "^1.12.1",
    "winston": "^2.1.1"
  }
}
