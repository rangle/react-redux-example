{
  "name": "react-redux-starter",
  "version": "0.1.0",
  "description": "A boiler-plate setup for React/Redux applications",
  "license": "MIT",
  "engines": {
    "node": "4.4.4"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf dist/ coverage/",
    "cover": "nyc check-coverage --lines 80 --functions 80 --branches 80",
    "dev:server": "cross-env NODE_ENV=development PORT=3000 nodemon --use_strict server/node-server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "dev": "concurrently -r 'npm run dev:client' 'npm run dev:server'",
    "lint": "npm run lint-js && npm run lint-css",
    "lint-css": "stylelint './src/**/*.css'",
    "lint-js": "eslint '**/*.js' --ignore-path .gitignore",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node --use_strict server/node-server.js",
    "pretest": "npm run clean coverage/ && npm run lint",
    "test": "nyc --reporter=lcov --reporter=text-summary mocha",
    "posttest": "nyc check-coverage",
    "test:watch": "mocha --watch"
  },
  "nyc": {
    "exclude": [
      "**/*.test.js",
      "test/entry.js"
    ],
    "lines": 80,
    "statements": 80,
    "branches": 80,
    "functions": 80
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "autoprefixer-core": "^6.0.1",
    "babel": "^6.5.2",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.1",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.11.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.9.0",
    "basscss": "^8.0.1",
    "basscss-addons": "^1.0.0-beta4",
    "basscss-align": "^1.0.2",
    "basscss-basic": "^1.0.0",
    "basscss-border": "^4.0.2",
    "basscss-flexbox": "^1.0.1",
    "basscss-grid": "^2.0.0",
    "basscss-hide": "^1.0.1",
    "basscss-layout": "^3.1.0",
    "basscss-margin": "^1.0.7",
    "basscss-padding": "^1.1.3",
    "basscss-position": "^2.0.3",
    "basscss-type-scale": "^1.0.5",
    "basscss-typography": "^3.0.3",
    "body-parser": "^1.15.1",
    "chai": "^3.4.0",
    "classnames": "^2.2.3",
    "concurrently": "^2.0.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.0",
    "cssnano": "^3.7.1",
    "denodeify": "^1.2.1",
    "enzyme": "^2.2.0",
    "es5-shim": "^4.5.6",
    "es6-promise": "^3.2.1",
    "es6-shim": "^0.35.0",
    "eslint": "3.0.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^5.2.2",
    "file-loader": "^0.9.0",
    "history": "^3.0.0",
    "html-webpack-plugin": "^2.9.0",
    "immutable": "^3.7.5",
    "json-loader": "^0.5.3",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2",
    "nyc": "^6.6.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-modules-local-by-default": "^1.0.2",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.2",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-form": "^5.2.3",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^2.1.4",
    "redux-thunk": "^2.0.1",
    "require-hacker": "^2.1.2",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.3",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "stylelint": "^6.8.0",
    "stylelint-webpack-plugin": "^0.2.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.6.4",
    "webpack-split-by-path": "0.0.10",
    "whatwg-fetch": "^1.0.0"
  },
  "dependencies": {
    "express": "^4.13.3",
    "helmet": "^2.0.0",
    "http-proxy": "^1.12.1",
    "winston": "^2.1.1"
  },
  "keywords": [
    "react",
    "redux",
    "starter",
    "seed"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rangle/react-redux-starter.git"
  },
  "bugs": {
    "url": "https://github.com/rangle/rangle-starter/issues"
  }
}
